[user]
	email = maweki@gmail.com
	name = Mario Wenzel
[core]
	editor = nano
[log]
	decorate = short
[push]
	default = current
[color]
	ui = auto
[credential]
	helper = cache
[alias]
	unstage = reset HEAD --
	a = add
	ap = add --patch
	b = branch
	l = log
	g = gui
	r = rebase
	ri = rebase --interactive
	d = diff
	s = status
	cm = commit -m
	rsv = remote --verbose show
	rvs = remote --verbose show
	pullr = pull --rebase
	undo = "!f() { \\\n    git reset --hard $(git rev-parse --abbrev-ref HEAD)@{${1-1}}; \\\n}; f"
[submodule]
	fetchJobs = 4
[diff]
	compactionHeuristic = true
